{
  "name": "stretching",
  "displayName": "Stretch",
  "publisher": "grg",
  "repository": {
    "type": "git",
    "url": "https://github.com/grgar/vscode-stretch"
  },
  "description": "Resize panels and sidebars with shortcuts",
  "icon": "images/icon.png",
  "version": "0.1.0",
  "license": "MIT",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Keymaps"
  ],
  "author": {
    "name": "grg",
    "url": "https://grg.app",
    "email": "vscode@grg.app"
  },
  "homepage": "https://github.com/grgar/vscode-stretch",
  "qna": "https://github.com/grgar/vscode-stretch/discussions",
  "bugs": {
    "email": "vscode@grg.app",
    "url": "https://github.com/grgar/vscode-stretch/issues"
  },
  "galleryBanner": {
    "theme": "light",
    "color": "#f6f8fa"
  },
  "contributes": {
    "keybindings": [
      {
        "key": "ctrl+alt+shift+left",
        "command": "workbench.action.increaseViewSize",
        "when": "auxiliaryBarFocus && !inputFocus && config.workbench.sideBar.location != 'right'"
      },
      {
        "key": "ctrl+alt+shift+right",
        "command": "workbench.action.increaseViewSize",
        "when": "sideBarFocus && !inputFocus && config.workbench.sideBar.location != 'right'"
      },
      {
        "key": "ctrl+alt+shift+left",
        "command": "workbench.action.decreaseViewSize",
        "when": "sideBarFocus && !inputFocus && config.workbench.sideBar.location != 'right'"
      },
      {
        "key": "ctrl+alt+shift+right",
        "command": "workbench.action.decreaseViewSize",
        "when": "auxiliaryBarFocus && !inputFocus && config.workbench.sideBar.location != 'right'"
      },
      {
        "key": "ctrl+alt+shift+right",
        "command": "workbench.action.increaseViewSize",
        "when": "auxiliaryBarFocus && !inputFocus && config.workbench.sideBar.location != 'right'"
      },
      {
        "key": "ctrl+alt+shift+left",
        "command": "workbench.action.increaseViewSize",
        "when": "sideBarFocus && !inputFocus && config.workbench.sideBar.location == 'right'"
      },
      {
        "key": "ctrl+alt+shift+right",
        "command": "workbench.action.decreaseViewSize",
        "when": "sideBarFocus && !inputFocus && config.workbench.sideBar.location == 'right'"
      },
      {
        "key": "ctrl+alt+shift+left",
        "command": "workbench.action.decreaseViewSize",
        "when": "auxiliaryBarFocus && !inputFocus && config.workbench.sideBar.location == 'right'"
      },
      {
        "key": "ctrl+alt+shift+up",
        "command": "workbench.action.increaseViewSize",
        "when": "panelFocus"
      },
      {
        "key": "ctrl+alt+shift+down",
        "command": "workbench.action.decreaseViewSize",
        "when": "panelFocus"
      }
    ]
  }
}
